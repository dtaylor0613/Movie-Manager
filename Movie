public class Movie {
    private String name;
    private double price;

    public Movie(String name, double price) {
        this.name = name;
        this.price = price;
    }

    public String getName() {
        return name;
    }

    public double getPrice() {
        return price;
    }

    @Override
    public String toString() {
        return "Movie{name='" + name + "', price=" + price + "}";
    }
}
